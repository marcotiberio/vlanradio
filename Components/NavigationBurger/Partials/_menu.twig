<ul class="menu" id="navigationBurger-menu">
  {% for item in items %}
    <a class="menu-item {{ item.current ? 'menu-item--current' }}" href="{{ item.link }}" {{ item.target == '_blank' ? 'target="_blank" rel="noreferrer noopener"' }}>
      <li class="menu-link">
          {{ item.title }}
      </li>
    </a>
  {% endfor %}
</ul>
